#ifndef LUABIND_VERSION_@luabind_VERSION@_HPP
#define LUABIND_VERSION_@luabind_VERSION@_HPP

#define LUABIND_VERSION         @luabind_VERSION@
#define LUABIND_VERSION_MAJOR   @luabind_VERSION_MAJOR@
#define LUABIND_VERSION_MINOR   @luabind_VERSION_MINOR@
#define LUABIND_VERSION_PATCH   @luabind_VERSION_PATCH@
#define LUABIND_VERSION_STRING  @luabind_VERSION_STRING@

// Each component uses two digits, so:
//
//   major = LUABIND_VERSION / 10000
//   minor = LUABIND_VERSION / 100 % 100
//   patch = LUABIND_VERSION % 100

#endif // LUABIND_VERSION_@luabind_VERSION@_HPP
